    "build:api": "tsc",
    "build": "yarn build:front && yarn build:api",
    "start": "node ./dist/api/index.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/lodash": "^4.17.20",
    "process": "^0.11.10"
  }
